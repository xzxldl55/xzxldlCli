{
  "presets": [
    "env",
    "react",
    "stage-0"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-decorators",
    "transform-react-constant-elements",
    "transform-react-inline-elements",
    [
      "transform-runtime",
      {
        "helpers": false,
        "ployfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
    
  ]
}
